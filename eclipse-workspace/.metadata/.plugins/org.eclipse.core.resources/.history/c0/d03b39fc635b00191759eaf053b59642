package edu.com;

public class Employee {
	private String name;
	private float rate;
	private float hours;
	
	public Employee(String name, float rate, float hours) {
		this.name = name;
		this.rate = rate;
		this.hours = hours;
	}

	public Employee(String name) {
		this.name = name;
	}
	
	public String getName() {
		return name;
	}
	
	public float getRate() {
		return rate;
	}
	
	
	
	
}
