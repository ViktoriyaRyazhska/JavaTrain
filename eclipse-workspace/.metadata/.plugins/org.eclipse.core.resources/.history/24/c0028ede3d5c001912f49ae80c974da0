
public class Person {
	private String Name;
	private int birthYear;
	
	public Person() {
	}


public Person (String Name, int birthYear) {
		this.Name = Name;
		this.birthYear = birthYear;
		
		
		@Override
		public String toString() {
			return "Person [Name=" + getName() + ", birthYear=" + birthYear + ", Age=" + Age + "]";
		}		
	
		public int getAge() {
			return 2019-bithYear;
		}
		
}
