
public class Person {
	private String name;
	private int birthYear;
	
	public Person() {
	}


public Person (String name, int birthYear) {
		this.name = name;
		this.birthYear = birthYear;
}	
		
		@Override
		public String toString() {
			return "Person [Name=" + name + ", birthYear=" + birthYear + "]";
		}		
	
		public int getAge() {
			return 2019-birthYear;
		}
		
}
